# -*- coding: utf-8 -*-

lista = []
for pos in range(0, 5):
    lista.append(int(input(f'Digite um número na posição {pos}: ')))    
print('\nO maior valor digitado foi {}, nas posições '.format(max(lista)), end='')
for p in range(0, 5):
    if lista[p] == max(lista):
        print(f'{p}...', end=' ')
print('\nO menor valor digitado foi {}, nas posições '.format(min(lista)), end='')
for p in range(0, 5):
    if lista[p] == min(lista):
        print(f'{p}...', end=' ')
