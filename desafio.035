#!/usr/bin/env python3
# -*- coding: utf-8 -*-

r1 = float(input('Primeira reta: '))
r2 = float(input('Segunda reta: '))
r3 = float(input('Terceira reta: '))
if (r1+r2)>r3 and (r2+r3)>r1 and (r1+r3)>r2:
    print('\n\033[32mÉ possível formar um triângulo com as três retas informadas.\033[m')
else:
    print('\n\033[31mNão é possível formar um triângulo com as retas informadas.\033[m')
print('\n--FIM--')
