# -*- coding: utf-8 -*-

print('\n*** SEQUÊNCIA DE FIBONACCI ***')
n = int(input('Digite quantos termos da sequência de Fibonacci deseja obter: '))
t1 = 0
t2 = 1
print('{} -> {}'.format(t1, t2), end='')
cont = 3 #já vai começar no terceiro termo pq os outros já estão no print acima.
while cont <= n:
    t3 = t1 + t2
    print(' -> {}'.format(t3), end='')
    t1 = t2
    t2 = t3
    cont += 1   
print('\n--FIM--')
